print("Welcome to the Love Calculator!")
name1 = input("What is your name? \n")
name2 = input("What is their name? \n")

combined = name1 + name2
lower_case = combined.lower()

t = lower_case.count("t")
r = lower_case.count("r")
u = lower_case.count("u")
e = lower_case.count("e")

true = t + r + u + e

l = lower_case.count("l")
o = lower_case.count("o")
v = lower_case.count("v")
e = lower_case.count("e")

love = l + o + v + e

score = int(str(true) + str(love))

if score < 10:
    print(f"Give it up not gonna happen, your score it just {score}...")
elif score > 90:
    print(f"Just get married already, your score is {score}")
else:
    print(f"Keep trying and one day you may be trully in love, score is {score}")
